{
  "name": "boutique",
  "version": "0.0.0",
  "licence": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lintter": "eslint .  --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prettier": "prettier --ext .vue --write . --ignore-path .gitignore",
    "test:unit": "vitest --environment jsdom --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "^7.4.0",
    "axios": "^0.27.2",
    "faker": "^6.6.6",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.7",
    "scss": "^0.2.4",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.30.1",
    "vue-tsc": "^0.34.7"
  }
}
